{
  "name": "instaclone",
  "version": "1.0.0",
  "description": "Instagram clone",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec babel-node src/server.js",
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "npm run deploy && npm run generate",
    "build": "babel src -d build",
    "postbuild": "cd src && copy ./api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawinderS/instaClone.git"
  },
  "author": "Dawinder Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dawinderS/instaClone/issues"
  },
  "homepage": "https://github.com/dawinderS/instaClone#readme",
  "dependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.11.2",
    "aws-sdk": "^2.729.0",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "graphql-tools": "^6.2.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.8",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "babel-cli": "^6.26.0",
    "nodemon": "^2.0.4"
  }
}
